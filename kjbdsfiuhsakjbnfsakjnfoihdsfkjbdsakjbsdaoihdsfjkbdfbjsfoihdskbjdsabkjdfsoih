local Window = OrionLib:MakeWindow({Name = "PhantomForces by unknown", HidePremium = false, SaveConfig = true, IntroEnabled = false, Icon = "https://i.pinimg.com/236x/d1/e3/00/d1e3003215f8298654cb9aa185fefa67.jpg", ConfigFolder = "MonkeysOnTop"})

